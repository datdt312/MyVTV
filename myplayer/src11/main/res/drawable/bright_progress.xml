<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#FF8d8b8b" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:centerColor="#168EF5"
                    android:endColor="#168EF5"
                    android:startColor="#168EF5" />
            </shape>
        </clip>
    </item>
</layer-list>